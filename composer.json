{
    "name": "riddlestone/brokkr-console",
    "description": "A Laminas module for Symfony Console",
    "keywords": [
        "laminas",
        "console"
    ],
    "homepage": "https://github.com/riddlestone/brokkr-console",
    "type": "library",
    "require": {
        "php": "^7.3",
        "container-interop/container-interop": "^1.2",
        "symfony/console": "^4.3",
        "laminas/laminas-component-installer": "^1.0 || ^0.7",
        "laminas/laminas-mvc": "^3.1",
        "laminas/laminas-servicemanager": "^3.4",
        "laminas/laminas-stdlib": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Riddlestone\\Brokkr\\Console\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4"
    },
    "autoload-dev": {
        "psr-4": {
            "Riddlestone\\Brokkr\\Console\\Test\\": "test/"
        }
    },
    "extra": {
        "laminas": {
            "component": "Riddlestone\\Brokkr\\Console"
        }
    },
    "bin": [
        "bin/brokkr"
    ],
    "scripts": {
        "test": "phpunit"
    },
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Andrew Riddlestone",
            "email": "andrew.riddlestone@gmail.com"
        }
    ]
}
